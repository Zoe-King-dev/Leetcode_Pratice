class Solution {
    public int[] findErrorNums(int[] nums) {
        Integer n = nums.length;
        int[] cnts = new int[n+1];
        for(Integer x:nums) cnts[x]++;
        int[] ans = new int[2];
        for (Integer i = 0; i <= n; i++){
            if(cnts[i] == 2) ans[0] = i;
            if(cnts[i] == 0) ans[1] = i;
        }
        return ans;
    }
}

