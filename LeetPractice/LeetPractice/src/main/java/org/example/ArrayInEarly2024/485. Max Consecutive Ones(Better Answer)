class Solution {
    public int findMaxConsecutiveOnes(int[] nums) {
        int count = 0, maxCount = 0;
        for(int item : nums){
            if(item==1){
                count++;
            }else{
                maxCount = Math.max(count, maxCount);
                count = 0;
            }
            maxCount = Math.max(count, maxCount);
        }
        return maxCount;
    }
}
